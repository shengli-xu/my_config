{
  "syncSettings.resources": [
    "extensions",
    "keybindings",
    "settings",
    "snippets",
    "uiState"
  ],
  "update.mode": "none",
  "workbench.activityBar.orientation": "vertical",
  "workbench.colorTheme": "Tokyo Night Storm",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.showTabs": "multiple",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": {
    "{hexdiff}:/**/*.*": "hexEditor.hexedit",
    "*.ipynb": "jupyter-notebook",
    "*.STL": "stlViewer.previewEditor",
    "*.svg": "default"
  },
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
    "statusBar.background": "#24283B",
    "statusBar.noFolderBackground": "#24283B",
    "statusBar.debuggingBackground": "#24283B",
    "statusBar.foreground": "#a9b1d6",
    "statusBar.debuggingForeground": "#a9b1d6"
  },
  "workbench.editor.empty.hint": "hidden",
  "editor.fontFamily": "JetBrain Mono",
  "editor.fontLigatures": true,
  "editor.fontWeight": "normal",
  "editor.cursorBlinking": "phase",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 200,
  "editor.fontSize": 14,
  "editor.lineHeight": 1.6,
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.formatOnSaveMode": "file",
  "editor.formatOnPaste": false,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.renderWhitespace": "none",
  "editor.renderControlCharacters": false,
  "editor.wordWrap": "off",
  "editor.suggestSelection": "first",
  "editor.wordWrapColumn": 79,
  "editor.rulers": [79],
  "editor.unicodeHighlight.nonBasicASCII": false,
  "editor.tabSize": 4,
  "editor.insertSpaces": true,
  "editor.detectIndentation": true,
  "editor.unicodeHighlight.allowedLocales": {
    "zh-hant": true
  },
  "editor.inlineSuggest.suppressSuggestions": true,
  "[xml]": {
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2,
    "editor.autoIndent": "keep",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.quickSuggestions": {
      "other": true,
      "comments": false,
      "strings": true
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[launch]": {
    "editor.tabSize": 2
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/.git": true,
    "**/.github": true,
    "**/.DS_Store": true,
    ".vscode": false,
    ".idea": true,
    "**/__pycache__": true,
    "**/**/*.pyc": true,
    ".python-version": true
  },
  "files.associations": {
    "*.launch": "xml"
  },
  "explorer.autoReveal": false,
  "explorer.openEditors.visible": 1,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "diffEditor.wordWrap": "off",
  "remote.SSH.configFile": "~/.ssh/config",
  "security.workspace.trust.untrustedFiles": "open",
  "breadcrumbs.enabled": false,
  "rewrap.autoWrap.enabled": true,
  "rewrap.wrappingColumn": 79,
  "security.workspace.trust.banner": "never",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.hideOnStartup": "always",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "terminal.integrated.fontSize": 14,
  "terminal.external.osxExec": "",
  "window.autoDetectHighContrast": false,
  "window.titleBarStyle": "custom",
  "update.showReleaseNotes": false,
  "debug.onTaskErrors": "showErrors",
  "debug.openDebug": "neverOpen",
  "extensions.ignoreRecommendations": true,
  "notebook.output.scrolling": true,
  "python.terminal.executeInFileDir": true,
  "python.languageServer": "None",
  "autopep8.args": ["--max-line-length=79", "--ignore", "E402"],
  "autopep8.path": ["${env:HOME}/.local/bin/autopep8"],
  "notebook.lineNumbers": "on",
  "notebook.cellToolbarVisibility": "hover",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "autoDocstring.docstringFormat": "numpy",
  "autoDocstring.startOnNewLine": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "[cpp]": {
    "editor.defaultFormatter": "llvm-vs-code-extensions.vscode-clangd"
  },
  "[tex]": {
    "editor.formatOnSave": false
  },
  "git.autofetch": true,
  "git.ignoreMissingGitWarning": true,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "git.openRepositoryInParentFolders": "never",
  "urdf-visualizer.packages": {
    "T170A": "${workspaceFolder}",
    "T170A.SLDASM": "${workspaceFolder}",
    "both_hands": "${workspaceFolder}",
    "inspire_hand_r": "${workspaceFolder}",
    "zy-hy-1.1-urdf": "${workspaceFolder}"
  },
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.hlsearch": true,
  "vim.normalModeKeyBindings": [
    {
      "before": ["<Leader>", "o"],
      "after": ["o", "<Esc>"]
    },
    {
      "before": ["<Leader>", "O"],
      "after": ["O", "<Esc>"]
    },
    {
      "before": ["<Esc>"],
      "commands": [":nohl"]
    }
  ],
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [":"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-a>": false,
    "<C-f>": false,
    "<C-b>": false,
    "<C-d>": true,
    "<C-s>": false,
    "<C-z>": false
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#24283B", "#a9b1d6"],
  "vim.statusBarColors.insert": ["#7AA2F7", "#24283B"],
  "vim.statusBarColors.visual": ["#E0AF68", "#24283B"],
  "vim.statusBarColors.visualline": ["#FF9E64", "#24283B"],
  "vim.statusBarColors.visualblock": ["#FF9E64", "#24283B"],
  "vim.statusBarColors.searchinprogressmode": ["#9ECE6A", "#24283B"],
  "vim.statusBarColors.replace": ["#7AA2F7", "#24283B"],
  "vim.mouseSelectionGoesIntoVisualMode": false,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 200,
  "[markdown]": {
    "editor.wordWrap": "off",
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "markdown.extension.preview.autoShowPreviewToSide": false,
  "projectManager.git.baseFolders": ["~/Projects", "~/zoyi"],
